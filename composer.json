{
    "name": "serafim/haxe-watcher",
    "description": "Haxe language watcher",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^1.1",
        "symfony/console": "^3.4",
        "symfony/process": "^3.4"
    },
    "autoload": {
        "classmap": [
            "src/out/lib"
        ],
        "psr-4": {
            "Serafim\\HaxeWatcher\\": "src"
        }
    },
    "extra": {
        "class": "Serafim\\HaxeWatcher\\Watcher"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
